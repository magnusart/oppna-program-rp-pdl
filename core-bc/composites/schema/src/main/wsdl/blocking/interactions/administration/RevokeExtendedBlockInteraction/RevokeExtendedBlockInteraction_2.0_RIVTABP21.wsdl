<wsdl:definitions name='RevokeExtendedBlockInteraction'
	xmlns:wsdl='http://schemas.xmlsoap.org/wsdl/'
	xmlns:soap='http://schemas.xmlsoap.org/wsdl/soap/'
	xmlns:xs='http://www.w3.org/2001/XMLSchema'
	xmlns:tjsr='urn:riv:ehr:blocking:administration:RevokeExtendedBlockResponder:2'
	xmlns:itr='urn:riv:itintegration:registry:1'
	xmlns:tns='urn:riv:ehr:blocking:administration:RevokeExtendedBlock:2:rivtabp21'
	targetNamespace='urn:riv:ehr:blocking:administration:RevokeExtendedBlock:2:rivtabp21'>

	<wsdl:documentation xml:lang='sv'>
		 Tjänsteinteraktionens namn: RevokeExtendedBlock
		 Beskrivning: 
		   Tjänst som häver en spärr permanent i den lokala spärrtjänsten, om spärren finns. Denna hävning kan inte återtas.
		   Tjänsten avregistrerar även spärren på nationell nivå.
		   Tjänsten realiseras på lokal nivå.
		 Revisioner: 
		 Tjänstedomän: blocking:administration
		 Tjänsteinteraktionstyp: Fråga-Svar
		 WS-profil: RIVTABP21
		 Förvaltas av: Inera AB
	</wsdl:documentation>
	
	<wsdl:types>
		<xs:schema targetNamespace='urn:riv:ehr:blocking:administration:RevokeExtendedBlock:2:rivtabp21'>
			<xs:import schemaLocation='RevokeExtendedBlockResponder_2.0.xsd' namespace='urn:riv:ehr:blocking:administration:RevokeExtendedBlockResponder:2' />
			<xs:import schemaLocation='../../../core_components/itintegration_registry_1.0.xsd' namespace='urn:riv:itintegration:registry:1' />
		</xs:schema>
	</wsdl:types>
	
		
	<wsdl:message name='RevokeExtendedBlockRequest'>
		<wsdl:part name='LogicalAddress' element='itr:LogicalAddress'>
			<wsdl:documentation xml:lang='sv'>Som logisk adress anges HSA-id för vårdgivaren som spärren gäller för.</wsdl:documentation>
		</wsdl:part>
	
		<wsdl:part name='parameters' element='tjsr:RevokeExtendedBlockRequest' />
	</wsdl:message>

	<wsdl:message name='RevokeExtendedBlockResponse'>
		<wsdl:part name='parameters' element='tjsr:RevokeExtendedBlockResponse' />
	</wsdl:message>

	<wsdl:portType name='RevokeExtendedBlockResponderInterface'>
		<wsdl:operation name='RevokeExtendedBlock'>
			<wsdl:input message='tns:RevokeExtendedBlockRequest' />
			<wsdl:output message='tns:RevokeExtendedBlockResponse' />
		</wsdl:operation>
	</wsdl:portType>

	<wsdl:binding name='RevokeExtendedBlockResponderBinding' type='tns:RevokeExtendedBlockResponderInterface'>
		<soap:binding style='document' transport='http://schemas.xmlsoap.org/soap/http' />
		<wsdl:operation name='RevokeExtendedBlock'>
			<soap:operation soapAction='urn:riv:ehr:blocking:administration:RevokeExtendedBlockResponder:2:RevokeExtendedBlock' style='document' />
			<wsdl:input>
				<soap:header use='literal' message='tns:RevokeExtendedBlockRequest' part='LogicalAddress' />
				<soap:body use='literal' parts='parameters' />
			</wsdl:input>
			<wsdl:output>
				<soap:body use='literal' />
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>

	<wsdl:service name='RevokeExtendedBlockResponderService'>
		<wsdl:port name='RevokeExtendedBlockResponderPort' binding='tns:RevokeExtendedBlockResponderBinding'>
			<soap:address location='http://tempuri.org' />
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>
