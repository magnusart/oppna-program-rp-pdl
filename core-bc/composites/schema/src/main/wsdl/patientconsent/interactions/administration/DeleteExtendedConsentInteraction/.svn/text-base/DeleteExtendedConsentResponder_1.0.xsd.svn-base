<xs:schema xmlns:xs='http://www.w3.org/2001/XMLSchema' 
	xmlns:tns='urn:riv:ehr:patientconsent:administration:DeleteExtendedConsentResponder:1'				
	xmlns:patientconsent='urn:riv:ehr:patientconsent:1'
	targetNamespace='urn:riv:ehr:patientconsent:administration:DeleteExtendedConsentResponder:1'
	elementFormDefault='qualified'
	attributeFormDefault='unqualified'
	version='1.0'>

	<xs:import schemaLocation='../../../core_components/ehr_patientconsent_1.0.xsd' namespace='urn:riv:ehr:patientconsent:1' />


	<xs:element name='DeleteExtendedConsentRequest' type='tns:DeleteExtendedConsentRequestType'/>

	<xs:complexType name='DeleteExtendedConsentRequestType'>
		<xs:annotation>
		    <xs:documentation xml:lang='sv'>
		        Tjänst som makulerar ett samtycke i samtyckestjänsten. Makulering av samtycke används enbart för borttagning av felregistrerade samtycken.        
		        Samtycket raderas inte från samtyckestjänst utan markeras som makulerad (ej längre giltig) för historikens skull. En makulering kan ej återtas.
		    </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AssertionId" type="patientconsent:Id"/>
			<xs:element name="DeletionAction" type="patientconsent:ActionType"/>
			<xs:any maxOccurs="unbounded" minOccurs="0" namespace="##other" processContents="lax"/>
		</xs:sequence>
	</xs:complexType>

	<xs:element name='DeleteExtendedConsentResponse' type='tns:DeleteExtendedConsentResponseType'/>

	<xs:complexType name='DeleteExtendedConsentResponseType'>
		<xs:annotation>
		    <xs:documentation xml:lang='sv'>
		        Tjänst som makulerar ett samtycke i samtyckestjänsten. Makulering av samtycke används enbart för borttagning av felregistrerade samtycken.        
		        Samtycket raderas inte från samtyckestjänst utan markeras som makulerad (ej längre giltig) för historikens skull. En makulering kan ej återtas.
		    </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ResultType" type="patientconsent:ResultType"/>
			<xs:any maxOccurs="unbounded" minOccurs="0" namespace="##other" processContents="lax"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
