<xs:schema xmlns:xs='http://www.w3.org/2001/XMLSchema' 
	xmlns:tns='urn:riv:ehr:patientrelationship:administration:DeleteExtendedPatientRelationResponder:1'				
	xmlns:patientrelationship='urn:riv:ehr:patientrelationship:1'
	targetNamespace='urn:riv:ehr:patientrelationship:administration:DeleteExtendedPatientRelationResponder:1'
	elementFormDefault='qualified'
	attributeFormDefault='unqualified'
	version='1.0'>

	<xs:import schemaLocation='../../../core_components/ehr_patientrelationship_1.0.xsd' namespace='urn:riv:ehr:patientrelationship:1' />


	<xs:element name='DeleteExtendedPatientRelationRequest' type='tns:DeleteExtendedPatientRelationRequestType'/>

	<xs:complexType name='DeleteExtendedPatientRelationRequestType'>
		<xs:annotation>
		    <xs:documentation xml:lang='sv'>
		        Tjänst som makulerar en patientrelation i patientrelationstjänsten. Makulering av patientrelation används enbart för borttagning av felregistrerade patientrelationer.
		        Patientrelationen raderas inte från patientrelationstjänsten utan markeras som makulerad (ej längre giltig) för historikens skull. En makulering kan ej återtas.
		    </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PatientRelationId" type="patientrelationship:Id"/>
			<xs:element name="DeletionAction" type="patientrelationship:ActionType"/>
			<xs:any maxOccurs="unbounded" minOccurs="0" namespace="##other" processContents="lax"/>
		</xs:sequence>
	</xs:complexType>

	<xs:element name='DeleteExtendedPatientRelationResponse' type='tns:DeleteExtendedPatientRelationResponseType'/>

	<xs:complexType name='DeleteExtendedPatientRelationResponseType'>
		<xs:annotation>
		    <xs:documentation xml:lang='sv'>
		        Tjänst som makulerar en patientrelation i patientrelationstjänsten. Makulering av patientrelation används enbart för borttagning av felregistrerade patientrelationer.
		        Patientrelationen raderas inte från patientrelationstjänsten utan markeras som makulerad (ej längre giltig) för historikens skull. En makulering kan ej återtas.
		    </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ResultType" type="patientrelationship:ResultType"/>
			<xs:any maxOccurs="unbounded" minOccurs="0" namespace="##other" processContents="lax"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
