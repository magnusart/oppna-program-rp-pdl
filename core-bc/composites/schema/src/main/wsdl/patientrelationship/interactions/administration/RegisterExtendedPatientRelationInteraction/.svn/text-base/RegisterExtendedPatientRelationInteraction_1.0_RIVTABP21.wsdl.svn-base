<wsdl:definitions name='RegisterExtendedPatientRelationInteraction'
	xmlns:wsdl='http://schemas.xmlsoap.org/wsdl/'
	xmlns:soap='http://schemas.xmlsoap.org/wsdl/soap/'
	xmlns:xs='http://www.w3.org/2001/XMLSchema'
	xmlns:tjsr='urn:riv:ehr:patientrelationship:administration:RegisterExtendedPatientRelationResponder:1'
	xmlns:itr='urn:riv:itintegration:registry:1'
	xmlns:tns='urn:riv:ehr:patientrelationship:administration:RegisterExtendedPatientRelation:1:rivtabp21'
	targetNamespace='urn:riv:ehr:patientrelationship:administration:RegisterExtendedPatientRelation:1:rivtabp21'>

	<wsdl:documentation xml:lang='sv'>
		 Tjänsteinteraktionens namn: RegisterExtendedPatientRelation
		 Beskrivning: 
		   Tjänst som registrerar en patientrelation mellan en patient och en medarbetare.
		   Tjänsten kräver utökad information (metainformation) kring skapande av intyget.
		 Revisioner: 
		 Tjänstedomän: patientrelationship:administration
		 Tjänsteinteraktionstyp: Fråga-Svar
		 WS-profil: RIVTABP21
		 Förvaltas av: Inera AB
	</wsdl:documentation>
	
	<wsdl:types>
		<xs:schema targetNamespace='urn:riv:ehr:patientrelationship:administration:RegisterExtendedPatientRelation:1:rivtabp21'>
			<xs:import schemaLocation='RegisterExtendedPatientRelationResponder_1.0.xsd' namespace='urn:riv:ehr:patientrelationship:administration:RegisterExtendedPatientRelationResponder:1' />
			<xs:import schemaLocation='../../../core_components/itintegration_registry_1.0.xsd' namespace='urn:riv:itintegration:registry:1' />
		</xs:schema>
	</wsdl:types>
	
		
	<wsdl:message name='RegisterExtendedPatientRelationRequest'>
		<wsdl:part name='LogicalAddress' element='itr:LogicalAddress'>
			<wsdl:documentation xml:lang='sv'>Som logisk adress anges HSA-id för vårdgivare som patientrelationen gäller för.</wsdl:documentation>
		</wsdl:part>
	
		<wsdl:part name='parameters' element='tjsr:RegisterExtendedPatientRelationRequest' />
	</wsdl:message>

	<wsdl:message name='RegisterExtendedPatientRelationResponse'>
		<wsdl:part name='parameters' element='tjsr:RegisterExtendedPatientRelationResponse' />
	</wsdl:message>

	<wsdl:portType name='RegisterExtendedPatientRelationResponderInterface'>
		<wsdl:operation name='RegisterExtendedPatientRelation'>
			<wsdl:input message='tns:RegisterExtendedPatientRelationRequest' />
			<wsdl:output message='tns:RegisterExtendedPatientRelationResponse' />
		</wsdl:operation>
	</wsdl:portType>

	<wsdl:binding name='RegisterExtendedPatientRelationResponderBinding' type='tns:RegisterExtendedPatientRelationResponderInterface'>
		<soap:binding style='document' transport='http://schemas.xmlsoap.org/soap/http' />
		<wsdl:operation name='RegisterExtendedPatientRelation'>
			<soap:operation soapAction='urn:riv:ehr:patientrelationship:administration:RegisterExtendedPatientRelationResponder:1:RegisterExtendedPatientRelation' style='document' />
			<wsdl:input>
				<soap:header use='literal' message='tns:RegisterExtendedPatientRelationRequest' part='LogicalAddress' />
				<soap:body use='literal' parts='parameters' />
			</wsdl:input>
			<wsdl:output>
				<soap:body use='literal' />
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>

	<wsdl:service name='RegisterExtendedPatientRelationResponderService'>
		<wsdl:port name='RegisterExtendedPatientRelationResponderPort' binding='tns:RegisterExtendedPatientRelationResponderBinding'>
			<soap:address location='http://tempuri.org' />
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>
